    "twitch-api-helix": "0.1.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "cross-env": "^7.0.3",
